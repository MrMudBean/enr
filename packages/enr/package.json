{
  "type": "module",
  "name": "enr",
  "version": "2.0.0-alpha.2",
  "description": "一个 earthnut 的 react ui",
  "scripts": {
    "vjj": "vjj",
    "diff": "jja pkg -d=淘宝",
    "push": "gvv",
    "clean": "jja rm node_modules/.cache .tsbuildinfo node_modules pnpm-lock.yaml && pnpm install && tsc --build --clean",
    "push:version": "gvv",
    "prepublishOnly": "pjj",
    "lint": "pwd && eslint . --fix",
    "build:css-pre": "node ./scripts/build-css.js pre",
    "build:css": "node ./scripts/build-css.js",
    "b": "pnpm run build:css-pre && rollup --config rollup.config.js && tsc -p tsconfig.build.json && pnpm run build:css",
    "build": "cd ../shared && pnpm build && cd ../core && jja rm dist run NODE_ENV=production 'pnpm run b' && node scripts/clean-package-json.js",
    "test": "cd ../shared && pnpm test && cd ../core && jja rm dist run NODE_ENV='development' 'pnpm run b' && cd ../preview && pnpm install",
    "build:local": "node ../../scripts/build-local.js"
  },
  "overrides": {
    "zza": "workspace:*"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "styled-component": "^2.6.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.6",
    "@qqi/state": "^0.0.0",
    "a-element-inline-style": "^1.0.1",
    "a-js-tools": "^2.0.1",
    "a-type-of-js": "^2.0.0",
    "xcn": "^1.1.0"
  },
  "devDependencies": {
    "zza": "workspace:*"
  },
  "sideEffect": [
    "*.scss",
    "*.css"
  ],
  "exports": {
    ".": {
      "default": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/es/index.js",
      "types": "./dist/es/src/index.d.ts"
    },
    "./common.scss": "./dist/styles/common.scss",
    "./common.css": "./dist/styles/common.css",
    "./reset.scss": "./dist/styles/reset.scss",
    "./reset.css": "./dist/styles/reset.css",
    "./styles/common.scss": "./dist/styles/common.scss",
    "./styles/common.css": "./dist/styles/common.css",
    "./styles/reset.scss": "./dist/styles/reset.scss",
    "./styles/reset.css": "./dist/styles/reset.css"
  }
}